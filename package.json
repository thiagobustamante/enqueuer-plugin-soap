{
  "name": "enqueuer-plugin-http-bind",
  "version": "1.0.2",
  "description": "The HTTP-Bind subscription provides implementations for an http service that can bind only some requests",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "start": "tsc -w",
    "build": "npm run clean && tsc && copyfiles -u 1 src/*.json dist",
    "clean": "rimraf dist",
    "lint": "tslint ./src/**/*.ts ./test/**/*.ts",
    "lint:fix": "tslint --fix ./src/**/*.ts ./test/**/*.ts -t verbose",
    "pretest": "cross-env NODE_ENV=test npm run build && npm run lint",
    "test": "cross-env NODE_ENV=test mocha --exit",
    "test:coverage": "nyc npm test",
    "tsc": "tsc"
  },
  "author": "Thiago da Rosa de Bustamante <thiago@cruxframework.org>",
  "keywords": [
    "enqueuer",
    "nqr",
    "plugin",
    "tests",
    "http",
    "integration Tests",
    "acceptance Tests"
  ],
  "license": "MIT",
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "json",
      "html"
    ],
    "report-dir": "./reports/coverage",
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "assert": "^2.0.0",
    "debug": "^4.1.1",
    "lodash": "^4.17.15",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/assert": "^1.4.3",
    "@types/bluebird": "^3.5.27",
    "@types/chai": "^4.2.0",
    "@types/cucumber": "^4.0.7",
    "@types/debug": "^4.1.5",
    "@types/express-serve-static-core": "^4.16.9",
    "@types/lodash": "^4.14.137",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.2",
    "@types/xml2js": "^0.4.4",
    "chai": "^4.2.0",
    "codecov": "^3.3.0",
    "copyfiles": "^2.1.1",
    "cross-env": "^5.2.0",
    "enqueuer": "^5.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "rimraf": "^2.7.1",
    "source-map-support": "^0.5.13",
    "ts-node": "^7.0.1",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "enqueuer": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thiagobustamante/enqueuer-plugin-http.git"
  },
  "bugs": {
    "url": "https://github.com/thiagobustamante/enqueuer-plugin-http/issues"
  },
  "directories": {
    "lib": "dist"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "engineStrict": true
}